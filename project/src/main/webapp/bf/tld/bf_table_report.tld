			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>bjFlag</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>bfReportTempTag</name>
		<tag-class>com.efraiser.bf.tag.BfTableReportTempTag</tag-class>
		<attribute>
			<name>reportId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>bjFlag</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
